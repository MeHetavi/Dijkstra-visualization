{
    "version": 2,
    "builds": [
        {
            "src": "app/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11.3"
            }
        },
        {
            "src": "static/**",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/wsgi.py"
        }
    ]
}